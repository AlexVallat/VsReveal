<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<AssemblyVersion>0.1</AssemblyVersion>

		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
		<AssemblyName>vs-reveal</AssemblyName>
		<Authors>Alex Vallat</Authors>
		<PackageProjectUrl>https://github.com/AlexVallat/VsReveal</PackageProjectUrl>
		<Copyright>$(PackageProjectUrl)</Copyright>
		<InformationalVersion>$(AssemblyVersion)</InformationalVersion>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>
    <PropertyGroup Condition="'$(PublishProtocol)'!=''">
		<DebugType>none</DebugType>
	</PropertyGroup>
    <ItemGroup>
		<None Remove=".gitignore" />
	</ItemGroup>
</Project>